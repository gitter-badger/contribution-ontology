# Class: :CRO_0000000 (Contribution Role)

AnnotationAssertion(rdfs:label :CRO_0000000 "Contribution Role")
SubClassOf(:CRO_0000000 owl:Thing)

# Class: :CRO_0000001 (Research Team Contribution Role)

AnnotationAssertion(rdfs:comment :CRO_0000001 "Contributions to the research team and project.")
AnnotationAssertion(rdfs:label :CRO_0000001 "Research Team Contribution Role")
SubClassOf(:CRO_0000001 :CRO_0000000)

# Class: :CRO_0000002 (Research Object Contribution Role)

AnnotationAssertion(rdfs:comment :CRO_0000002 "Contributions to the published research object")
AnnotationAssertion(rdfs:label :CRO_0000002 "Research Object Contribution Role")
SubClassOf(:CRO_0000002 :CRO_0000000)

# Class: :CRO_0000003 (Conceptualization Role)

AnnotationAssertion(rdfs:comment :CRO_0000003 "Ideas; formulation or evolution of overarching research goals and aims.")
AnnotationAssertion(rdfs:label :CRO_0000003 "Conceptualization Role")
SubClassOf(:CRO_0000003 :CRO_0000001)

# Class: :CRO_0000004 (Methodology Role)

AnnotationAssertion(rdfs:comment :CRO_0000004 "Development or design of methodology; creation of models.")
AnnotationAssertion(rdfs:label :CRO_0000004 "Methodology Role")
SubClassOf(:CRO_0000004 :CRO_0000001)

# Class: :CRO_0000005 (Software Development Role)

AnnotationAssertion(rdfs:comment :CRO_0000005 "Programming, software development; designing computer programs; implementation of the computer code and supporting algorithms; testing of existing code components.")
AnnotationAssertion(rdfs:label :CRO_0000005 "Software Development Role")
SubClassOf(:CRO_0000005 :CRO_0000001)

# Class: :CRO_0000006 (Validation Role)

AnnotationAssertion(rdfs:comment :CRO_0000006 "Verification, whether as a part of the activity or separate, of the overall replication/reproducibility of results/experiments and other research outputs.")
AnnotationAssertion(rdfs:label :CRO_0000006 "Validation Role")
SubClassOf(:CRO_0000006 :CRO_0000001)

# Class: :CRO_0000007 (Formal Analysis Role)

AnnotationAssertion(rdfs:comment :CRO_0000007 "Application of statistical, mathematical, computational, or other formal techniques to analyse or synthesize study data.")
AnnotationAssertion(rdfs:label :CRO_0000007 "Formal Analysis Role")
SubClassOf(:CRO_0000007 :CRO_0000001)

# Class: :CRO_0000008 (Investigation Role)

AnnotationAssertion(rdfs:comment :CRO_0000008 "Conducting a research and investigation process, specifically performing the experiments, or data/evidence collection.")
AnnotationAssertion(rdfs:label :CRO_0000008 "Investigation Role")
SubClassOf(:CRO_0000008 :CRO_0000001)

# Class: :CRO_0000009 (Resource Provision Role)

AnnotationAssertion(rdfs:comment :CRO_0000009 "Provision of study materials, reagents, materials, patients, laboratory samples, animals, instrumentation, computing resources, or other analysis tools.")
AnnotationAssertion(rdfs:label :CRO_0000009 "Resource Provision Role")
SubClassOf(:CRO_0000009 :CRO_0000001)

# Class: :CRO_0000010 (Data Curation Role)

AnnotationAssertion(rdfs:comment :CRO_0000010 "Management activities to annotate (produce metadata), scrub data and maintain research data (including software code, where it is necessary for interpreting the data itself) for initial use and later re-use.")
AnnotationAssertion(rdfs:label :CRO_0000010 "Data Curation Role")
SubClassOf(:CRO_0000010 :CRO_0000001)

# Class: :CRO_0000011 (Supervision Role)

AnnotationAssertion(rdfs:comment :CRO_0000011 "Oversight and leadership responsibility for the research activity planning and execution, including mentorship external to the core team.")
AnnotationAssertion(rdfs:label :CRO_0000011 "Supervision Role")
SubClassOf(:CRO_0000011 :CRO_0000001)

# Class: :CRO_0000012 (Project Administration Role)

AnnotationAssertion(rdfs:comment :CRO_0000012 "Management and coordination responsibility for the research activity planning and execution.")
AnnotationAssertion(rdfs:label :CRO_0000012 "Project Administration Role")
SubClassOf(:CRO_0000012 :CRO_0000001)

# Class: :CRO_0000013 (Funding Acquisition Role)

AnnotationAssertion(rdfs:comment :CRO_0000013 "Acquisition of the financial support for the project leading to this publication.")
AnnotationAssertion(rdfs:label :CRO_0000013 "Funding Acquisition Role")
SubClassOf(:CRO_0000013 :CRO_0000001)

# Class: :CRO_0000014 (Team Management Role)

AnnotationAssertion(rdfs:comment :CRO_0000014 "Coordination of diverse team members, often across different disciplines and locations.")
AnnotationAssertion(rdfs:label :CRO_0000014 "Team Management Role")
SubClassOf(:CRO_0000014 :CRO_0000001)

# Class: :CRO_0000015 (Data Entry Role)

AnnotationAssertion(rdfs:comment :CRO_0000015 "Manual entry or automated ingestion.")
AnnotationAssertion(rdfs:label :CRO_0000015 "Data Entry Role")
SubClassOf(:CRO_0000015 :CRO_0000001)

# Class: :CRO_0000016 (Science Communication Role)

AnnotationAssertion(rdfs:comment :CRO_0000016 "Coordination of research dissemination through popular media and the press, as well as Community Outreach and liaising of project directly to the community.")
AnnotationAssertion(rdfs:label :CRO_0000016 "Science Communication Role")
SubClassOf(:CRO_0000016 :CRO_0000001)

# Class: :CRO_0000017 (Background and Literature Search Role)

AnnotationAssertion(rdfs:comment :CRO_0000017 "Includes literature searches and contributions to review activities (systematic reviews, scoping reviews, etc.)")
AnnotationAssertion(rdfs:label :CRO_0000017 "Background and Literature Search Role")
SubClassOf(:CRO_0000017 :CRO_0000001)

# Class: :CRO_0000018 (Regulatory Administration Role)

AnnotationAssertion(rdfs:comment :CRO_0000018 "Manage regulatory and compliance issues for the research project.")
AnnotationAssertion(rdfs:label :CRO_0000018 "Regulatory Administration Role")
SubClassOf(:CRO_0000018 :CRO_0000001)

# Class: :CRO_0000019 (Writing Original Draft Role)

AnnotationAssertion(rdfs:comment :CRO_0000019 "Preparation, creation and/or presentation of the published work, specifically writing the initial draft (including substantive translation).")
AnnotationAssertion(rdfs:label :CRO_0000019 "Writing Original Draft Role")
SubClassOf(:CRO_0000019 :CRO_0000002)

# Class: :CRO_0000020 (Reviewing and Editing Manuscript Role)

AnnotationAssertion(rdfs:comment :CRO_0000020 "Preparation, creation and/or presentation of the published work by those from the original research group, specifically critical review, commentary or revision â€“ including pre- or post-publication stages.")
AnnotationAssertion(rdfs:label :CRO_0000020 "Reviewing and Editing Manuscript Role")
SubClassOf(:CRO_0000020 :CRO_0000002)

# Class: :CRO_0000021 (Visualization Role)

AnnotationAssertion(rdfs:comment :CRO_0000021 "Preparation, creation and/or presentation of the published work, specifically visualization/data presentation.")
AnnotationAssertion(rdfs:label :CRO_0000021 "Visualization Role")
SubClassOf(:CRO_0000021 :CRO_0000002)

# Class: :CRO_0000022 (Preservation Role)

AnnotationAssertion(rdfs:comment :CRO_0000022 "Coordination of the deposit of the manuscript, data, and any secondary materials to appropriate repositories; ensure that outputs comply with OA mandates.")
AnnotationAssertion(rdfs:label :CRO_0000022 "Preservation Role")
SubClassOf(:CRO_0000022 :CRO_0000002)


